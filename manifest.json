{
	"name": "Antisocial",
	"version": "0.2.0",
	"description": "Blocks social plugins from loading",
	"icons": {
		"128": "128.png",
		"48": "48.png",
		"16": "16.png"
	},
	"content_scripts": [{
		"all_frames": true,
		"js": ["antisocial.js"],
		"matches": ["http://*/*", "https://*/*"],
		"exclude_globs": [
			"*addthis.com*", 
			"*digg.com*", 
			"*facebook.com*", 
			"*google.com*", 
			"*klout.com*", 
			"*linkedin.com*", 
			"*meebo.com*", 
			"*reddit.com*",
			"*sharethis.com*", 
			"*stumbleupon.com*", 
			"*tumblr.com*", 
			"*twitter.com*", 
			"*wibiya.com*"
		],
		"run_at": "document_start"
	}],
	"background_page" : "background.html",
	"browser_action": {
		"default_icon": "icons/19x19.png",
		"default_title": "Antisocial",
		"default_popup": "popup.html"
	}
}